<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout8"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PatientPortal_FoodsAvoid">

    <!-- TODO: Update blank fragment layout -->

    <ImageButton
        android:id="@+id/image_account_settings_patportal_fta"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="6dp"
        android:adjustViewBounds="true"
        android:background="#0000"
        android:scaleType="fitCenter"
        android:src="@drawable/account_settings"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/image_med_meals"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/doc_portal_heading_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="112dp"
        android:gravity="center|center_horizontal"
        android:text="@string/pat_portal"
        android:textAlignment="center"
        android:textColor="@color/light_blue_buttons"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/foods_to_avoid_text"
        android:layout_width="322dp"
        android:layout_height="34dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="180dp"
        android:text="Foods to Avoid"
        android:background="@drawable/custom_border_bottom"
        android:textAlignment="viewStart"
        android:textColor="@color/light_blue_buttons"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="411dp"
        android:layout_height="69dp"
        android:layout_marginBottom="1dp"
        android:background="#ECEEEF"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"></LinearLayout>

    <ImageView
        android:id="@+id/image_med_meals"
        android:layout_width="232dp"
        android:layout_height="66dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/medmeals_logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/linearLayout"
        android:layout_width="411dp"
        android:layout_height="69dp"
        android:layout_marginTop="75dp"
        android:layout_marginBottom="1dp"
        android:background="@drawable/custom_border"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageButton
            android:id="@+id/button_patient_ftapage_meal_recs"
            android:layout_width="60dp"
            android:layout_height="57dp"
            android:layout_marginBottom="4dp"
            android:src="@drawable/doc_notes_meal_recs"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.15"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/button_patient_ftapage_doc_notes"
            android:layout_width="73dp"
            android:layout_height="55dp"
            android:src="@drawable/dark_blue_meal_recs2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.49"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.571"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/button_patient_ftapage_foods_to_avoid"
            android:layout_width="71dp"
            android:layout_height="55dp"
            android:layout_marginStart="41dp"
            android:src="@drawable/fta_dark_blue"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/button_patient_ftapage_doc_notes"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="SpeakableTextPresentCheck" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/fta_text_1"
        android:layout_width="223dp"
        android:layout_height="65dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="5dp"
        android:background="#3644BCBC"
        android:gravity="center_vertical"
        android:text="TextView"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="@color/dark_blue_buttons"
        android:textSize="22sp"
        app:layout_constraintBottom_toTopOf="@+id/fta_text_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/doc_portal_heading_3" />

    <TextView
        android:id="@+id/fta_text_2"
        android:layout_width="223dp"
        android:layout_height="65dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="5dp"
        android:background="#1A44BCBC"
        android:gravity="center_vertical"
        android:text="TextView"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="@color/dark_blue_buttons"
        android:textSize="22sp"
        app:layout_constraintBottom_toTopOf="@+id/fta_text_3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fta_text_1" />

    <TextView
        android:id="@+id/fta_text_3"
        android:layout_width="223dp"
        android:layout_height="65dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="5dp"
        android:background="#3644BCBC"
        android:gravity="center"
        android:text="TextView"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="@color/dark_blue_buttons"
        android:textSize="22sp"
        app:layout_constraintBottom_toTopOf="@+id/fta_text_4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fta_text_2" />

    <TextView
        android:id="@+id/fta_text_4"
        android:layout_width="223dp"
        android:layout_height="65dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="30dp"
        android:background="#1A44BCBC"
        android:gravity="center_vertical"
        android:text="TextView"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="@color/dark_blue_buttons"
        android:textSize="22sp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fta_text_3"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/fta_image_1"
        android:layout_width="112dp"
        android:layout_height="84dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="2dp"
        app:layout_constraintBottom_toTopOf="@+id/fta_image2"
        app:layout_constraintEnd_toStartOf="@+id/fta_text_1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/foods_to_avoid_text"
        app:layout_constraintVertical_bias="0.0"
        tools:src="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/fta_image2"
        android:layout_width="112dp"
        android:layout_height="84dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toTopOf="@+id/fta_image3"
        app:layout_constraintEnd_toStartOf="@+id/fta_text_2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fta_image_1"
        tools:src="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/fta_image3"
        android:layout_width="112dp"
        android:layout_height="84dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toTopOf="@+id/fta_image_4"
        app:layout_constraintEnd_toStartOf="@+id/fta_text_3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fta_image2"
        tools:src="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/fta_image_4"
        android:layout_width="112dp"
        android:layout_height="84dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toStartOf="@+id/fta_text_4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fta_image3"
        tools:src="@tools:sample/avatars" />

    <TextView
        android:id="@+id/no_fta"
        android:layout_width="353dp"
        android:layout_height="72dp"
        android:gravity="center"
        android:text="You don't have any foods to avoid entered by your doctors yet."
        android:textColor="@color/dark_blue_buttons"
        android:textSize="20sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.517"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.361" />


</androidx.constraintlayout.widget.ConstraintLayout>